{
  "name": "ava-ui",
  "version": "3.3.3",
  "description": "AVA Neural Interface - Cortex-Medulla Architecture",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
